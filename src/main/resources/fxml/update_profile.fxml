<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="service.UpdateProfileController">
    <children>
        <Label layoutX="50.0" layoutY="40.0" text="Full Name" />
        <TextField fx:id="fullNameField" layoutX="150.0" layoutY="40.0" />
        <Label layoutX="50.0" layoutY="80.0" text="Email" />
        <TextField fx:id="emailField" layoutX="150.0" layoutY="80.0" />
        <Button layoutX="150.0" layoutY="140.0" text="Update" onAction="#handleUpdateProfile" />
    </children>
</AnchorPane>
